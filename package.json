    "type": "git",
    "url": "git+https://github.com/exploropleth/binguru.git"
  },
  "dependencies": {
    "nerdamer": "^1.1.13",
    "simple-statistics": "^7.8.3"
  },
  "keywords": [
    "data",
    "binning;",
    "data",
    "classification",
    "choropleth",
    "maps",
    "GIS",
    "cartography",
    "visualization"
  ],
